package com.pack.model;



import org.hibernate.annotations.Entity;


@Entity
public class Customer 
{
	@Id
	private int custid;
	private String name;
	private String address;
	private int age;
	private float salery;
	
	public Customer() {
		// TODO Auto-generated constructor stub
	}
	
	
	
	public Customer(int custid, String name, String address, int age, float salery) {
		super();
		this.custid = custid;
		this.name = name;
		this.address = address;
		this.age = age;
		this.salery = salery;
	}
	
	
	public Customer(int custid, String address, int age, float salery) {
		super();
		this.custid = custid;
		this.address = address;
		this.age = age;
		this.salery = salery;
	}


	public int getCustid() {
		return custid;
	}
	public void setCustid(int custid) {
		this.custid = custid;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public float getSalery() {
		return salery;
	}
	public void setSalery(float salery) {
		this.salery = salery;
	}

}
