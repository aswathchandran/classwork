package com.pack;

import org.apache.log4j.Logger;

public class Main1 {
	static final Logger logger =Logger.getLogger(Main.class);
	public static void main(String[] a) 
	{
		logger.debug("sample debug");
		logger.error("error");
		logger.fatal("fatel");
		logger.info("info");
		logger.warn("warn");
	}
}
